{
    "bracketSpacing": true,
    "bracketSameLine": true,
    "jsxSingleQuote": true,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 3,
    "importOrder": [
        "^react",
        "^[store/]",
        "^[common/]",
        "^[features/]",
        "^utils/(.*)$",
        "^[assets/]",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
